{
  "name": "mtg-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "NODE_ENV=test next dev --port 3001",
    "build": "npx prisma migrate deploy && prisma generate && next build",
    "start": "next start",
    "lint": "next lint && npm run typecheck",
    "typecheck": "tsc --noEmit --pretty",
    "docker": "docker compose up -d",
    "purgedb": "npx prisma migrate reset",
    "e2e": "start-server-and-test test http://localhost:3001 \"playwright test\"",
    "e2e:ui": "start-server-and-test test http://localhost:3001 \"playwright test --ui\""
  },
